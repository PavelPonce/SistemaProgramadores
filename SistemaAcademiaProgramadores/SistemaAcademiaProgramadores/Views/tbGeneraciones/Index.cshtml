@model IEnumerable<SistemaAcademiaProgramadores.Models.tbGeneraciones>

@{
    ViewBag.Title = $"tbGeneraciones".Substring(2);
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<section class="content">

<div class="card">
<div class="card-header">
<h3 class="card-title">@ViewBag.Title</h3>
<div class="card-tools">
<button type="button" class="btn btn-tool" data-card-widget="collapse" title="Collapse">
<i class="fa fa-minus"></i>
</button>
<button type="button" class="btn btn-tool" data-card-widget="remove" title="Remove">
<i class="fa fa-times"></i>
</button>
</div>
</div>
<div class="card-body">
    <div class="card-title">
        <a href="@Url.Action("Create")" class="btn btn-primary col-sm-12 mb-4">
            <i class="fa fa-plus-square-o"> </i> Nuevo
        </a>
    </div>

        
    <table class="table table-responsive table-striped table-bordered">
        <tr class="thead-purple thead-justify-content-center">
                    <th>
                @Html.DisplayNameFor(model => model.Gener_Nombre)
            </th>
                    <th>
                @Html.DisplayNameFor(model => model.Gener_Anhio)
            </th>
                    <th>
                @Html.DisplayNameFor(model => model.Gener_FechaCreacion)
            </th>
                    <th>
                @Html.DisplayNameFor(model => model.Gener_FechaModificacion)
            </th>
                    <th>
                @Html.DisplayNameFor(model => model.Gener_Estado)
            </th>
                    <th>
                @Html.DisplayNameFor(model => model.Gener_FechaInicio)
            </th>
                    <th>
                @Html.DisplayNameFor(model => model.Gener_FechaFin)
            </th>
                        <th>
                @Html.DisplayNameFor(model => model.tbUsuarios.Usuar_Usuario)
            </th>
                    <th>
                @Html.DisplayNameFor(model => model.tbUsuarios1.Usuar_Usuario)
            </th>
                
            <th></th>
           
        </tr>

    @foreach (var item in Model) {
        <tr>
                    <td>
                @Html.DisplayFor(modelItem => item.Gener_Nombre)
            </td>
                    <td>
                @Html.DisplayFor(modelItem => item.Gener_Anhio)
            </td>
                    <td>
                @Html.DisplayFor(modelItem => item.Gener_FechaCreacion)
            </td>
                    <td>
                @Html.DisplayFor(modelItem => item.Gener_FechaModificacion)
            </td>
                    <td>
                @Html.DisplayFor(modelItem => item.Gener_Estado)
            </td>
                    <td>
                @Html.DisplayFor(modelItem => item.Gener_FechaInicio)
            </td>
                    <td>
                @Html.DisplayFor(modelItem => item.Gener_FechaFin)
            </td>
                        <td>
                @Html.DisplayFor(modelItem => item.tbUsuarios.Usuar_Usuario)
            </td>
                    <td>
                @Html.DisplayFor(modelItem => item.tbUsuarios1.Usuar_Usuario)
            </td>
                <td class="col-sm-6">
                <a class="btn btn-editar col-sm-2" href="@Url.Action("Edit")">
                    <i class="fa fa-edit"></i>
                </a>
                <a class="btn btn-danger col-sm-2" href="@Url.Action("Delete")">
                    <i class="fa fa-trash-o"></i>
                </a>
                <a class="btn btn-detalle col-sm-2" href="@Url.Action("Details")">
                    <i class="fa fa-eye"></i>
                </a>
            </td>
           
            </tr>
    }

    </table>
</div>

<div class="card-footer">

</div>

</div>

</section>


